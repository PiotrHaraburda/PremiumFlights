<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="110.0" prefWidth="260.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.PremiumFlights.Logout">
   <children>
      <Label fx:id="infoText" alignment="CENTER" layoutX="14.0" layoutY="22.0" prefHeight="20.0" prefWidth="232.0" text="Czy napewno chcesz się wylogować?">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="yesButton" layoutX="55.0" layoutY="63.0" mnemonicParsing="false" onAction="#onYesClick" prefHeight="25.0" prefWidth="62.0" text="Tak" textFill="#87732f">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="noButton" layoutX="144.0" layoutY="63.0" mnemonicParsing="false" onAction="#onNoClick" prefHeight="25.0" prefWidth="62.0" text="Nie" textFill="#87732f">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
